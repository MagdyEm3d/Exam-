<Page x:Class="Holistic_Exam.Sign_in"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Holistic_Exam"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Sign_in" Background="White">

    <Grid>
        <TextBox x:Name="textbox1" HorizontalAlignment="Left" Margin="333,105,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Username" HorizontalAlignment="Left" Margin="183,105,0,0" VerticalAlignment="Top" Width="92"/>
        <Label Content="password" HorizontalAlignment="Left" Margin="195,140,0,0" VerticalAlignment="Top" Width="80"/>
        <Button Content="signin" HorizontalAlignment="Center" Margin="0,225,0,0" VerticalAlignment="Top" Width="105" Click="Button_Click"/>
        <PasswordBox Name="password" HorizontalAlignment="Left" Margin="333,148,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Page>
